{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/email/cron/execute",
      "schedule": "25 0 * * *"
    },
    {
      "path": "/api/v1/email/cron/execute",
      "schedule": "25 16 * * *"
    }
  ]
}
